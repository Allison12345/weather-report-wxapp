{
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "parser": "babel",
  "overrides": [
    {
      "files": "*.html",
      "options": { "parser": "html" }
    },
    {
      "files": "*.json",
      "options": { "parser": "json" }
    },
    {
      "files": "*.prettierrc",
      "options": { "parser": "json" }
    },
    {
      "files": "*.css",
      "options": { "parser": "css" }
    }
  ]
}
