<view class="air-index-info">
  <view class="air-index-info__bd">
    <span class="air-icon" bindtap='onInfoClose'>✖️</span>
    <view class="air-index-info__bd-content">
      <view class="air-title">空气质量指数</view>
      <view class="air-number">{{ air }}</view>
      <view class="air-level">
        <air-index air_level="{{ air_level }}"></air-index>
      </view>
  </view>
  </view>
  <view class="air-index-info__ft">{{ air_tips }}</view>
</view>
