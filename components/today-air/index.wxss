.today-air{
    width: 100%;
    padding: 10px;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 5px;
    box-sizing: border-box;
}
.today-icon{
    width: 24px;
    height: 24px;
}
.today-air-right{
    display: flex;
}
.today-air-left__txt{
margin-left: 10px;
padding-top: 5px;
font-size: 12px;
}